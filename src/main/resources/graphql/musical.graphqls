type Musical
{
    id: ID!,
    imageSource: String,
    name: String,
    theatre: Theatre,
    performances: [Performance]!,
    producers: [Producer]!,
    actors: [Actor]!,
    directors: [Director]!,
    musicalTypes: [MusicalType]!,
    musicalGenres: [MusicalGenre]!,
    musicalAtmospheres: [MusicalAtmosphere]!
}

input SortInput
{
    musicals: [Int!]!
    ascending: Boolean!
}